<%- include('../partials/header') %>
<div class="carContainer">
    <% cars.forEach((car,index)=>{%>
    <div class="formContent">
        <h3>Name: <%= car.name %></h3>
        <h3>Make: <%= car.make %></h3>
        <h3>Model: <%= car.model %></h3>
        <h3>Color: <%= car.color %></h3>
        <a href="/cars/<%= car._id %>">
            <button class="addButton">
            More info
            </button>
        </a>
    </div>
        <% }) %>
</div>
<%- include('../partials/footer') %>
